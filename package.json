{
  "name": "@thisisagile/easy-sample",
  "version": "0.1.0",
  "description": "Straightforward set of examples for building domain-driven microservice architectures using @thisisagile/easy",
  "author": "Sander Hoogendoorn",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index",
  "repository": {
    "type": "git",
    "url": "git@github.com:thisisagile/easy-sample.git"
  },
  "keywords": [
    "typescript",
    "microservices",
    "domain-driven-design",
    "evolutionay-architecture",
    "examples"
  ],
  "scripts": {
    "build": "tsc",
    "pretest": "jest --clearCache",
    "test": "jest --coverage",
    "prestart": "npm run build",
    "start": "node -r .",
    "quick": "ts-node -r dotenv/config src/service",
    "prepack": "npm run build",
    "semantic-release": "semantic-release"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@thisisagile/easy": "^1.16.1",
    "@thisisagile/easy-test": "^1.5.2",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "ts-node": "^9.0.0",
    "typescript": "4.1.2"
  },
  "dependencies": {
    "@types/express": "^4.17.9",
    "express": "^4.17.1"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.14.8"
  }
}
